{
	"manifest_version": 2,

	"name": "Magento Orders",
	"description": "Magento chrome extension for downloading orders.",
	"version": "1.0",
	"options_page": "options.html",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Magento Orders"
	},

	"permissions": [
		"<all_urls>"
	],

	"background": {
		"scripts":["background.js"]
	}
}